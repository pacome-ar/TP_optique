#!/srv/conda/envs/notebook/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'astropy==4.0.1','console_scripts','fitscheck'
__requires__ = 'astropy==4.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('astropy==4.0.1', 'console_scripts', 'fitscheck')()
    )
